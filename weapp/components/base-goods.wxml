
<view class="goods-container">
  <view class="goods-wrapper">
    <view wx:for="{{ 7 }}" wx:for-index="index" wx:for-item="item" wx:key="index" key="{{ item.name }}" data-wpy-evt="9-8" bindtap="_proxy">
      <van-card bind_init="_initComponent" custom-class="van-card" lazy-load="true" price="2.00" origin-price="198.00" desc="新用户数、客单价、重复消费、转介绍" title="11模块、37大价值，引爆营销力" thumb="{{ imageURL }}" link-type="switchTab" tag="免费">
        <view slot="tags" class="tags">
          <van-tag bind_init="_initComponent" type="success" class="tag">免费</van-tag>
          <van-tag bind_init="_initComponent" type="warning" class="tag">可试看</van-tag>
        </view>
      </van-card>
    </view>
  </view>
</view>
