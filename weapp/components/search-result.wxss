.search-result-container {
  box-sizing: border-box;
  margin-top: 50px;
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  z-index: 100;
  background: #fff;
  padding: 7px 13px;
}
.search-result-container .search-result .search-result-item {
  width: 100%;
  height: 35px;
  line-height: 35px;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1px solid #f5f5f5;
}
.search-result-container .search-result .search-result-item .icon {
  flex: 0 0;
}
.search-result-container .search-result .search-result-item .text {
  flex: 1 1;
  font-size: 12px;
  padding-left: 10px;
  color: #666;
}

