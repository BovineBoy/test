
<view wx:if="{{ searchResultShow }}" class="search-result-container">
  <view wx:if="{{ searchResultList }}" class="search-result">
    <view wx:for="{{ searchResultList }}" wx:for-index="index" wx:for-item="item" wx:key="index" key="{{ item.id }}" class="search-result-item">
      <label class="icon icon-left"><van-icon bind_init="_initComponent" size="14px" name="search"></van-icon></label>
      <label class="text">{{item.content}}</label>
      <label class="icon icon-right"><van-icon bind_init="_initComponent" size="14px" name="arrow"></van-icon></label>
    </view>
  </view>
  <view wx:else>
    暂无搜索结果
  </view>
</view>
