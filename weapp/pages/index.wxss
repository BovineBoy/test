.search-model {
  margin: 10px 0 5px 0;
  width: 100%;
  height: 34px;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  color: #323233;
  padding: 0 10px;
}
.search-model .search-input {
  flex: 2;
  box-sizing: border-box;
  align-items: center;
  height: 34px;
  display: flex;
  background: #f7f8fa;
  padding-left: 30px;
  font-size: 14px;
  border-radius: 3px;
  color: #999;
}
.search-model .search-text {
  font-size: 14px;
  margin-left: 10px;
  text-align: center;
  display: block;
}

